function showforgotpw(){$("#forgotpwwindow").dialog("open")}function showregister(){$("#registerwindow").dialog("open")}
function submitregister(){$.post("<?php echo base_url();?>index.php/users/register",{account:$("#registeraccount").val(),password:$("#registerpw").val(),confirm:$("#registerpwconfirm").val(),email:$("#registeremail").val()},function(a,b){"success"==b?"success"==a.status?($("#registerwindow").dialog("close"),alert("\u8a3b\u518a\u6210\u529f\uff0c\u8acb\u8f38\u5165\u5e33\u865f\u5bc6\u78bc")):alert(a.data):alert("\u8a3b\u518a\u9023\u7dda\u5931\u6557")})}
function submitforgotpw(){$.post("<?php echo base_url();?>index.php/users/forgotpw",{account:$("#forgotpwaccount").val()},function(a,b){"success"==b?"success"==a.status?($("#registerwindow").dialog("close"),alert("\u5df2\u7d93\u5c07\u5bc6\u78bc\u5bc4\u5230\u8a72\u5e33\u865f\u7d81\u5b9a\u4e4b\u4fe1\u7bb1")):alert(a.data):alert("\u9023\u7dda\u5931\u6557")})}
$(document).ready(function(){$("#forgotpwwindow").dialog({autoOpen:!1,height:"auto",width:"auto",modal:!0});$("#registerwindow").dialog({autoOpen:!1,height:"auto",width:"auto",modal:!0})});
